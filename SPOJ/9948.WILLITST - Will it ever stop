#include <bits/stdc++.h>

using namespace std;

typedef long long int lli;


int main(){
    lli number;
    bool stop = true;
    cin >> number;
    while(number>1){
        if (number%2==1){
            stop = false;
            break;
        }
        number = number/2;

    }

    if (stop)
        cout  << "TAK" << endl;
    else
        cout << "NIE" << endl;
}
